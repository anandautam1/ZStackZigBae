    311          /*********************************************************************
    312           * @fn      _itoa
    313           *
    314           * @brief   convert a 16bit number to ASCII
    315           *
    316           * @param   num -
    317           *          buf -
    318           *          radix -
    319           *
    320           * @return  void
    321           *
    322           *********************************************************************/
    323          void _itoa(uint16 num, uint8 *buf, uint8 radix)
    324          {
    325            char c,i;
    326            uint8 *p, rst[5];
    327          
    328            p = rst;
    329            for ( i=0; i<5; i++,p++ )
    330            {
    331              c = num % radix;  // Isolate a digit
    332              *p = c + (( c < 10 ) ? '0' : '7');  // Convert to Ascii
    333              num /= radix;
    334              if ( !num )
    335                break;
    336            }
    337          
    338            for ( c=0 ; c<=i; c++ )
    339              *buf++ = *p--;  // Reverse character order
    340          
    341            *buf = '\0';
    342          }
