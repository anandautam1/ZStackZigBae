    403          
    404              /* Build and send back the response */
    405              MT_BuildAndSendZToolResponse(((uint8)MT_RPC_CMD_SRSP | (uint8)MT_RPC_SYS_SYS), cmdId, respLen, pRetBuf );
    406          
    407              osal_mem_free(pRetBuf);
