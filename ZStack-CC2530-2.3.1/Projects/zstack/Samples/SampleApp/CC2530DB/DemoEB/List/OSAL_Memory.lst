    177          /*********************************************************************
    178           * @fn      osal_mem_init
    179           *
    180           * @brief   Initialize the heap memory management system.
    181           *
    182           * @param   void
    183           *
    184           * @return  void
    185           */
    186          void osal_mem_init( void )
    187          {
    188            osalMemHdr_t *tmp;
    189          
    190          #if ( OSALMEM_PROFILER )
    191            (void)osal_memset( theHeap, OSALMEM_INIT, MAXMEMHEAP );
    192          #endif
    193          
    194            // Setup a NULL block at the end of the heap for fast comparisons with zero.
    195            tmp = (osalMemHdr_t *)theHeap + (MAXMEMHEAP / HDRSZ) - 1;
    196            *tmp = 0;
    197          
    198            // Setup a small-block bucket.
    199            tmp = (osalMemHdr_t *)theHeap;
    200            *tmp = SMALLBLKHEAP;
    201          
    202            // Setup the wilderness.
    203            tmp = (osalMemHdr_t *)theHeap + (SMALLBLKHEAP / HDRSZ);
    204            *tmp = ((MAXMEMHEAP / HDRSZ) * HDRSZ) - SMALLBLKHEAP - HDRSZ;
    205          
    206            // Setup a NULL block that is never freed so that the small-block bucket
    207            // is never coalesced with the wilderness.
    208            ff1 = tmp;
    209            ff2 = osal_mem_alloc( 0 );
    210            ff1 = (osalMemHdr_t *)theHeap;
    211          
    212          #if ( OSALMEM_METRICS )
    213            /* Start with the small-block bucket and the wilderness - don't count the
    214             * end-of-heap NULL block nor the end-of-small-block NULL block.
    215             */
    216            blkCnt = blkFree = 2;
    217          #endif
    218          }
    219          
    220          /*********************************************************************
    221           * @fn      osal_mem_kick
    222           *
    223           * @brief   Kick the ff1 pointer out past the long-lived OSAL Task blocks.
    224           *          Invoke this once after all long-lived blocks have been allocated -
    225           *          presently at the end of osal_init_system().
    226           *
    227           * @param   void
    228           *
    229           * @return  void
    230           */
    231          void osal_mem_kick( void )
    232          {
    233            halIntState_t intState;
    234          
    235            HAL_ENTER_CRITICAL_SECTION( intState );  // Hold off interrupts.
    236          
    237            /* Logic in osal_mem_free() will ratchet ff1 back down to the first free
    238             * block in the small-block bucket.
    239             */
    240            ff1 = ff2;
    241          
    242            HAL_EXIT_CRITICAL_SECTION( intState );  // Re-enable interrupts.
    243          }
