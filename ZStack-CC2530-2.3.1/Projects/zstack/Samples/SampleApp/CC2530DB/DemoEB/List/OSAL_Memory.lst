    427          /*********************************************************************
    428           * @fn      osal_mem_free
    429           *
    430           * @brief   Implementation of the de-allocator functionality.
    431           *
    432           * @param   ptr - pointer to the memory to free.
    433           *
    434           * @return  void
    435           */
    436          void osal_mem_free( void *ptr )
    437          {
    438            osalMemHdr_t *currHdr;
    439            halIntState_t intState;
    440          
    441            HAL_ENTER_CRITICAL_SECTION( intState );  // Hold off interrupts.
    442          
    443            HAL_ASSERT(((uint8 *)ptr >= (uint8 *)_theHeap) && ((uint8 *)ptr < (uint8 *)_theHeap+MAXMEMHEAP));
    444          
    445            currHdr = (osalMemHdr_t *)ptr - 1;
    446          
    447            HAL_ASSERT(*currHdr & OSALMEM_IN_USE);
    448          
    449            *currHdr &= ~OSALMEM_IN_USE;
    450          
    451          #if ( OSALMEM_PROFILER )
    452            {
    453              uint16 size = *currHdr;
    454              uint8 idx;
    455          
    456              for ( idx = 0; idx < OSALMEM_PROMAX; idx++ )
    457              {
    458                if ( size <= proCnt[idx] )
    459                {
    460                  break;
    461                }
    462              }
    463          
    464              proCur[idx]--;
    465            }
    466          #endif
    467          
    468          #if ( OSALMEM_METRICS )
    469            memAlo -= *currHdr;
    470            blkFree++;
    471          #endif
    472          
    473            if ( ff1 > currHdr )
    474            {
    475              ff1 = currHdr;
    476            }
    477          
    478          #if ( OSALMEM_PROFILER )
    479            (void)osal_memset( (uint8 *)currHdr+HDRSZ, OSALMEM_REIN, (*currHdr - HDRSZ) );
    480          #endif
    481          
    482            HAL_EXIT_CRITICAL_SECTION( intState );  // Re-enable interrupts.
    483          }
