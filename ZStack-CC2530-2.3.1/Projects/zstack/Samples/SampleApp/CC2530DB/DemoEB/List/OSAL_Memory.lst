    245          /*********************************************************************
    246           * @fn      osal_mem_alloc
    247           *
    248           * @brief   Implementation of the allocator functionality.
    249           *
    250           * @param   size - number of bytes to allocate from the heap.
    251           *
    252           * @return  void * - pointer to the heap allocation; NULL if error or failure.
    253           */
    254          void *osal_mem_alloc( uint16 size )
    255          {
    256            osalMemHdr_t *prev = NULL;
    257            osalMemHdr_t *hdr;
    258            halIntState_t intState;
    259            uint16 tmp;
    260            uint8 coal = 0;
    261          
    262            OSALMEM_ASSERT( size );
    263          
    264            size += HDRSZ;
    265          
    266            // Calculate required bytes to add to 'size' to align to halDataAlign_t.
    267            if ( sizeof( halDataAlign_t ) == 2 )
    268            {
    269              size += (size & 0x01);
    270            }
    271            else if ( sizeof( halDataAlign_t ) != 1 )
    272            {
    273              const uint8 mod = size % sizeof( halDataAlign_t );
    274          
    275              if ( mod != 0 )
    276              {
    277                size += (sizeof( halDataAlign_t ) - mod);
    278              }
    279            }
    280          
    281            HAL_ENTER_CRITICAL_SECTION( intState );  // Hold off interrupts.
    282          
    283            // Smaller allocations are first attempted in the small-block bucket.
    284            if ( size <= OSALMEM_SMALL_BLKSZ )
    285            {
    286              hdr = ff1;
    287            }
    288            else
    289            {
    290              hdr = ff2;
    291            }
    292            tmp = *hdr;
    293          
    294            do
    295            {
    296              if ( tmp & OSALMEM_IN_USE )
    297              {
    298                tmp ^= OSALMEM_IN_USE;
    299                coal = 0;
    300              }
    301              else
    302              {
    303                if ( coal != 0 )
    304                {
    305          #if ( OSALMEM_METRICS )
    306                  blkCnt--;
    307                  blkFree--;
    308          #endif
    309          
    310                  *prev += *hdr;
    311          
    312                  if ( *prev >= size )
    313                  {
    314                    hdr = prev;
    315                    tmp = *hdr;
    316                    break;
    317                  }
    318                }
    319                else
    320                {
    321                  if ( tmp >= size )
    322                  {
    323                    break;
    324                  }
    325          
    326                  coal = 1;
    327                  prev = hdr;
    328                }
    329              }
    330          
    331              hdr = (osalMemHdr_t *)((uint8 *)hdr + tmp);
    332          
    333              tmp = *hdr;
    334              if ( tmp == 0 )
    335              {
    336                hdr = NULL;
    337                break;
    338              }
    339          
    340          
    341            } while ( 1 );
    342          
    343            if ( hdr != NULL )
    344            {
    345              tmp -= size;
    346          
    347              // Determine whether the threshold for splitting is met.
    348              if ( tmp >= OSALMEM_MIN_BLKSZ )
    349              {
    350                // Split the block before allocating it.
    351                osalMemHdr_t *next = (osalMemHdr_t *)((uint8 *)hdr + size);
    352                *next = tmp;
    353                *hdr = (size | OSALMEM_IN_USE);
    354          
    355          #if ( OSALMEM_METRICS )
    356                blkCnt++;
    357                if ( blkMax < blkCnt )
    358                {
    359                  blkMax = blkCnt;
    360                }
    361                memAlo += size;
    362          #endif
    363              }
    364              else
    365              {
    366          #if ( OSALMEM_METRICS )
    367                memAlo += *hdr;
    368                blkFree--;
    369          #endif
    370          
    371                *hdr |= OSALMEM_IN_USE;
    372              }
    373          
    374          #if ( OSALMEM_METRICS )
    375              if ( memMax < memAlo )
    376              {
    377                memMax = memAlo;
    378              }
    379          #endif
    380          
    381          #if ( OSALMEM_PROFILER )
    382            {
    383              uint8 idx;
    384              size = *hdr ^ OSALMEM_IN_USE;
    385          
    386              for ( idx = 0; idx < OSALMEM_PROMAX; idx++ )
    387              {
    388                if ( size <= proCnt[idx] )
    389                {
    390                  break;
    391                }
    392              }
    393              proCur[idx]++;
    394              if ( proMax[idx] < proCur[idx] )
    395              {
    396                proMax[idx] = proCur[idx];
    397              }
    398              proTot[idx]++;
    399            }
    400          #endif
    401          
    402              hdr++;
    403          
    404          #if ( OSALMEM_PROFILER )
    405              (void)osal_memset( (uint8 *)hdr, OSALMEM_ALOC, (size - HDRSZ) );
    406          
    407              /* A small-block could not be allocated in the small-block bucket.
    408               * When this occurs significantly frequently, increase the size of the
    409               * bucket in order to restore better worst case run times. Set the first
    410               * profiling bucket size in proCnt[] to the small-block bucket size and
    411               * divide proSmallBlkMiss by the corresponding proTot[] size to get % miss.
    412               * Best worst case time on TrasmitApp was achieved at a 0-15% miss rate
    413               * during steady state Tx load, 0% during idle and steady state Rx load.
    414               */
    415              if ( (size <= OSALMEM_SMALL_BLKSZ) && (hdr > ff2) )
    416              {
    417                proSmallBlkMiss++;
    418              }
    419          #endif
    420            }
